<h1 class="user_header"><%=@museum.name%> Reviews</h1>

<% @museum.reviews.each do |review|%>
  <h2><%= link_to review.title, review_path %></h2>
  <h3>Rating: <%=review.rating%></h3>
  <p> <%= review.review %></p>
  <h4>Reviewer: <%= link_to review.user.username, user_path(review.user) %></h4>
  <h4>Date Written: <%=review.date_written %></h4>
  <br>
  <br>
<%end%>
